<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
		  http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd"
         version="2.1">
  <enterprise-beans>
      <session>
          <ejb-name>CreatingXmlEJB</ejb-name>
          <home>ru.elleriumsoft.xml.creatingxml.CreatingXmlHome</home>
          <remote>ru.elleriumsoft.xml.creatingxml.CreatingXml</remote>
          <ejb-class>ru.elleriumsoft.xml.creatingxml.CreatingXmlBean</ejb-class>
          <session-type>Stateless</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>ObjectOfStructureEJB</ejb-name>
          <home>ru.elleriumsoft.structure.object.ObjectOfStructureHome</home>
          <remote>ru.elleriumsoft.structure.object.ObjectOfStructure</remote>
          <ejb-class>ru.elleriumsoft.structure.object.ObjectOfStructureBean</ejb-class>
          <session-type>Stateful</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>ObjectDeptEJB</ejb-name>
          <home>ru.elleriumsoft.department.object.ObjectDeptHome</home>
          <remote>ru.elleriumsoft.department.object.ObjectDept</remote>
          <ejb-class>ru.elleriumsoft.department.object.ObjectDeptBean</ejb-class>
          <session-type>Stateful</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>ChangeItemDepartmentEJB</ejb-name>
          <home>ru.elleriumsoft.department.modification.ChangeItemDepartmentHome</home>
          <remote>ru.elleriumsoft.department.modification.ChangeItemDepartment</remote>
          <ejb-class>ru.elleriumsoft.department.modification.ChangeItemDepartmentBean</ejb-class>
          <session-type>Stateless</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>ExportEJB</ejb-name>
          <home>ru.elleriumsoft.xml.exchange.export.ExportHome</home>
          <remote>ru.elleriumsoft.xml.exchange.export.Export</remote>
          <ejb-class>ru.elleriumsoft.xml.exchange.export.ExportBean</ejb-class>
          <session-type>Stateless</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <session>
          <ejb-name>ObjectFinderEJB</ejb-name>
          <home>ru.elleriumsoft.finder.object.ObjectFinderHome</home>
          <remote>ru.elleriumsoft.finder.object.ObjectFinder</remote>
          <ejb-class>ru.elleriumsoft.finder.object.ObjectFinderBean</ejb-class>
          <session-type>Stateless</session-type>
          <transaction-type>Container</transaction-type>
      </session>
      <entity>
          <ejb-name>StructureProcessingFromDbEJB</ejb-name>
          <home>ru.elleriumsoft.structure.entity.StructureProcessingFromDbHome</home>
          <remote>ru.elleriumsoft.structure.entity.StructureProcessingFromDb</remote>
          <ejb-class>ru.elleriumsoft.structure.entity.StructureProcessingFromDbBean</ejb-class>
          <persistence-type>Bean</persistence-type>
          <prim-key-class>java.lang.Integer</prim-key-class>
          <reentrant>false</reentrant>
      </entity>
      <entity>
          <ejb-name>EntityDeptEJB</ejb-name>
          <home>ru.elleriumsoft.department.entity.EntityDeptHome</home>
          <remote>ru.elleriumsoft.department.entity.EntityDept</remote>
          <ejb-class>ru.elleriumsoft.department.entity.EntityDeptBean</ejb-class>
          <persistence-type>Bean</persistence-type>
          <prim-key-class>java.lang.Integer</prim-key-class>
          <reentrant>true</reentrant>
      </entity>
      <entity>
          <ejb-name>EntityOccupationEJB</ejb-name>
          <home>ru.elleriumsoft.occupation.entity.EntityOccupationHome</home>
          <remote>ru.elleriumsoft.occupation.entity.EntityOccupation</remote>
          <ejb-class>ru.elleriumsoft.occupation.entity.EntityOccupationBean</ejb-class>
          <persistence-type>Bean</persistence-type>
          <prim-key-class>java.lang.Integer</prim-key-class>
          <reentrant>true</reentrant>
      </entity>
      <entity>
          <ejb-name>EntityFinderEJB</ejb-name>
          <home>ru.elleriumsoft.finder.entity.EntityFinderHome</home>
          <remote>ru.elleriumsoft.finder.entity.EntityFinder</remote>
          <ejb-class>ru.elleriumsoft.finder.entity.EntityFinderBean</ejb-class>
          <persistence-type>Bean</persistence-type>
          <prim-key-class>java.lang.Integer</prim-key-class>
          <reentrant>true</reentrant>
      </entity>
  </enterprise-beans>
</ejb-jar>
